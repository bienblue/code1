#include <stdio.h>
void nhap (float&a,float&b);
float Thuong (float a, float b,);
void xuat (float x);
void main ()
{
    float a,b;
    nhap (a,b);
    xuat (Thuong(a,b));
 }   
void nhap (float &a , float &b);
{
    scanf ("%f%f",&a,&b );
}
  float Thuong (float a, float b)
{ 
   return a/b ; 
} 
void xuat (float x)
{
  printf ("%2f",x);
}
