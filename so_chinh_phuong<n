#include<stdio.h>

void main() 
{

          int n, i, j;
       do {

                   printf("n: ");

                   scanf("%d", &n);

          } while(n<=0);

 

          printf("\n: ");

          for(i=1; i<=n; i++) {

                   for(j=1; j<=i; j++) {

                             if(j*j==i)

                                      printf(" %d", i);

                   }

          }
}
